@model IEnumerable<DOICMS.Models.ComplaintListItem>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "ComplaintCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ComplaintID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InvestigatorName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AgentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InsurerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConsumerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ComplaintDesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Resolved)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateSubmitted)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateCompleted)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.ComplaintID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.InvestigatorName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.OrderType)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.AgentName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.InsurerName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ConsumerName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ComplaintDesc)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Resolved)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DateSubmitted)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DateCompleted)
    </td>
    <td>
        @Html.ActionLink("Edit", "ComplaintEdit", new { id = item.ComplaintID }) |
        @Html.ActionLink("Details", "ComplaintDetails", new { id = item.ComplaintID }) |
        @Html.ActionLink("Delete", "ComplaintDelete", new { id = item.ComplaintID })
    </td>
</tr>
}

</table>
